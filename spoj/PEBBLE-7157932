#include<stdio.h>

int main()

{
 
int i,j=1,count;

char a[100000];

 while(scanf("%s",a)!=EOF)
{
 	

 	count=0;

	for(i=0;a[i]!='\0';i++)

	{

		if(a[i]=='1')

		{

			if(count%2==0)

				count++;

		}

		else

		{

			if(count%2==1)

				count++;

		}

	}
	
	printf("Game #%d: %d\n",j++,count);

 }

return 0;

}
