#include<stdio.h>
#include<math.h>
main()
{
        int n,i,j,x;
        long int a;
        scanf("%d\n",&n);
        for(i=1;i<=n;i++)
        {       x=0;
                j=1;
                scanf("%ld",&a);        
                while(pow(5,j)<=a)
                {
                        x=x+(a/pow(5,j));
                        j++;
                }
                printf("%d\n",x);
        }
return 0;
}  