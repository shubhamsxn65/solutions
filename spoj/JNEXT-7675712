#include<stdio.h>
int num[1000010];
int main()
{
	int t,n,i,j,temp,left,right,f;
	scanf("%d",&t);
	while(t--)
	{
		f=0;
		scanf("%d",&n);
		for(i=0;i<n;i++)
			scanf("%d",&num[i]);
		for(i=n-1;i>0;i--)
		{
			if(num[i]>num[i-1])
			{
				f=1;
				break;
			}
		}
		i--;
		if(f==0)
		{
			printf("-1\n");
			continue;
			}
		for(j=n-1;j>i;j--)
		{
			if(num[j]>num[i])
			{
				temp=num[i];
				num[i]=num[j];
				num[j]=temp;
				break;
			}
		}
		left=i+1;
		right=n-1;
		while(left<right)
		{
			temp=num[left];
			num[left]=num[right];
			num[right]=temp;
			left++;
			right--;
			}
		for(i=0;i<n;i++)
			printf("%d",num[i]);
		printf("\n");
		}
	return(0);
}