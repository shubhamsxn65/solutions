#include<stdio.h>
int main()
{
	int a[10],s,i;
		for(i=0;i<10;i++)
			scanf("%d",(a+i));
		s=0;
		for(i=0;i<10;i++)
		{
			s=s+a[i];
			if(s>=100)
				break;
		}
		if(s==100)
			printf("%d\n",s);
		else if(s>100)
		{
			if((100-(s-a[i]))>=s-100)
				printf("%d\n",s);
			else if((100-(s-a[i]))<s-100)
				printf("%d\n",s-a[i]);
		}
		else
			printf("%d\n",s);
	return 0;
}
