#include<iostream>
#include<stdio.h>
using namespace std;
int k,count,m,prime[15],ans;
void call(int sign,int no,int id)
{
	int i;
	if(id>k)
		return ;
	if(sign==1)
	{
		if(no!=1)
			count-=(m/no);
		for(i=id;i<k;i++)
		call(0,no*prime[i],i+1);
	}
	else
	{
		if(no!=1)
			count+=(m/no);
		for(i=id;i<k;i++)
		call(1,no*prime[i],i+1);
	}
}
int get_int()
{
	char c;
	ans=0;
	c=getc_unlocked(stdin);
	while(c>32)
	{
		ans=ans*10 +(c-48);
		c=getc_unlocked(stdin);
	}
	return ans;
}
int main()
{
	int n,t,i;
	t=get_int();
	while(t--)
	{
		n=get_int();
		m=get_int();
		k=0;count=0;
		for(i=2;i*i<=n;i++)
		{
			if(n%i==0)
			{
				prime[k++]=i;
				while(!(n%i))
					n=n/i;
			}
		}
		if(n>1)
			prime[k++]=n;
		/*for(i=0;i<k;i++)
			printf("%d\n",prime[i]);*/
		call(1,1,0);
		printf("%d\n",m-count);
	}
	return 0;
}